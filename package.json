{
    "name": "angitignored",
    "displayName": "AnGitIgnored",
    "description": "Tool that facilitates the management of .ignore files",
    "license": "MIT",
    "preview": true,
    "icon": "Readme_images/logoAnGitIgnoredV00000904.png",
    "galleryBanner": {
        "color": "#003152",
        "theme": "dark"
    },
    "version": "1.2.17",
    "publisher": "AnAppWiLos",
    "engines": {
        "vscode": "^1.22.0"
    },
    "keywords": [
        "git",
        "gitHub",
        "ignore",
        "gitignore",
        ".gitignore",
        "gitignore.io",
        "gitignore generator"
    ],
    "categories": [
        "SCM Providers",
        "Programming Languages",
        "Formatters",
        "Snippets",
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/nicolasalarconrapela/angitignore"
    },
    "homepage": "https://github.com/nicolasalarconrapela/angitignore",
    "bugs": {
        "url": "https://github.com/nicolasalarconrapela/angitignore/labels/bug"
    },
    "qna": "https://stackoverflow.com/questions/tagged/vscode-extensions",
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.addGitignored",
                "title": "Add GitIgnored : ‚ûïAdd to .gitignore"
            },
            {
                "command": "extension.hideGitignored",
                "title": "Hide GitIgnored: üëªHide files ignored by .gitignore"
            },
            {
                "command": "extension.showGitignored",
                "title": "Show GitIgnored: üëÄShow files ignored by .gitignore"
            },
            {
                "command": "extension.generateGitignored",
                "title": "Generate GitIgnored: üìùGenerate the file .gitignore"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "extension.addGitignored",
                    "group": "navigation"
                },
                {
                    "command": "extension.hideGitignored",
                    "group": "navigation"
                },
                {
                    "command": "extension.showGitignored",
                    "group": "navigation"
                },
                {
                    "command": "extension.generateGitignored",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.6.8",
        "tslint": "^5.18.0",
        "typescript": "^3.5.3",
        "vscode": "^1.1.35"
    },
    "dependencies": {
        "vsce": "^1.66.0"
    }
}
